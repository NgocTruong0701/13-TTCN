
@{
    ViewBag.Title = "Tinh";
}

<h2>Thông tin tiền điện</h2>
<table class="table table-bordered table-striped">
    <tr>
        <td>Chủ hộ:</td>
        <td>@ViewBag.Name</td>
    </tr>
    <tr>
        <td>Số điện tiêu thụ: </td>
        <td>@ViewBag.SoDien</td>
    </tr>
    <tr>
        <td>Tiền phải trả: </td>
        <td>@ViewBag.SoTienTra</td>
    </tr>
</table>

